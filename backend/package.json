{
  "name": "herramienta_tutores",
  "version": "1.0.0",
  "description": "Proyecto para GPS 2025-1",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "vitest run",
    "test:c": "vitest run --coverage",
    "test:w": "vitest --watch",
    "test:ch": "vitest run --coverage --reporter html",
    "test:ui": "vitest --ui",
    "test:ui:u": "vitest ui test/unit",
    "test:ui:i": "vitest ui test/integration --sequence.concurrent=false",
    "test:i": "vitest run test/integration --sequence.concurrent=false",
    "test:ui:all": "npm-run-all --sequential test:ui:u test:ui:i",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "author": "Marcelo Paz Pezo",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-session": "^1.18.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.5",
    "multer": "^2.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "supertest": "^7.1.4",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "eslint": "^9.9.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "vitest": "^3.1.4"
  }
}
